import java.io.*;

public class NameArrayTest {

	/**
	 * @param args
	 */
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] name = {"Joe", "Bloggs"};
		char c;
		System.out.println(name);
		
		c = name.charAt(3);
		System.out.println("4 character is: " + c);
		
	}

}
