import java.io.*;

public class NameArrayTest {

	/**
	 * @param args
	 */
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] myName = {"Joe", "Bloggs"};
		char c;
		System.out.println(myName);
		c = myName.charAt[0];
		System.out.println("4 character is: " + c);
		
	}

}
